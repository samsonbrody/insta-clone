<% if(success && success.length) { %>
<div
  class="alert alert-dark alert-dismissible fade show"
  style="background-color: #8a3ab964"
  role="alert"
>
  <%= success %>
  <button
    type="button"
    class="btn-close"
    data-bs-dismiss="alert"
    aria-label="Close"
  ></button>
</div>
<% } %> <% if(error && error.length) { %>
<div
  class="alert alert-dark alert-dismissible fade show"
  style="background-color: #8a3ab964"
  role="alert"
>
  <%= error %>
  <button
    type="button"
    class="btn-close"
    data-bs-dismiss="alert"
    aria-label="Close"
  ></button>
</div>
<% } %>
